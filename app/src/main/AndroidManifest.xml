<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jixstreet.temanusaha">

    <permission
        android:name="com.jixstreet.temanusaha.permission.MAPS_RECEIVE"
        android:protectionLevel="signature" />

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="com.jixstreet.temanusaha.permission.MAPS_RECEIVE" />

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <!-- [START gcm_permission] -->
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />
    <uses-permission android:name="com.google.android.c2dm.permission.SEND" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="com.jixstreet.temanusaha.permission.C2D_MESSAGE" />
    <!-- [END gcm_permission] -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <permission android:name="com.jixstreet.temanusaha.permission.C2D_MESSAGE" android:protectionLevel="signature" />

    <uses-permission android:name="com.jixstreet.temanusaha.permission.C2D_MESSAGE" />
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_GPS" />
    <uses-permission android:name="android.permission.ACCESS_LOCATION" />

    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />

    <application
        android:name="com.jixstreet.temanusaha.TemanUsahaApplication"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity android:name="com.jixstreet.temanusaha.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.jixstreet.temanusaha.LoginActivity" />
        <activity android:name="com.jixstreet.temanusaha.MainActivity" />
        <activity android:name="com.jixstreet.temanusaha.TermsActivity" />
        <activity android:name="com.jixstreet.temanusaha.ChangePasswordActivity" />
        <activity android:name="com.jixstreet.temanusaha.CustomerProfileActivity" />
        <activity android:name="com.jixstreet.temanusaha.PickShapeActivity" />
        <activity android:name="com.jixstreet.temanusaha.RegisterProfileActivity" />
        <activity android:name="com.jixstreet.temanusaha.ShowMapActivity" />
        <activity android:name="com.jixstreet.temanusaha.CountDownActivity" />
        <activity android:name="com.jixstreet.temanusaha.PartnerConfirmationActivity" />
        <activity android:name="com.soundcloud.android.crop.CropImageActivity" />
        <activity android:name="com.jixstreet.temanusaha.DeliveryDataConfirmationActivity" />
        <activity android:name="com.jixstreet.temanusaha.AdministrationProcessActivity" />
        <activity android:name="com.jixstreet.temanusaha.MeetUpProcessActivity" />
        <activity android:name="com.jixstreet.temanusaha.Main" />
        <activity android:name="com.jixstreet.temanusaha.GetIntent" />
        <activity android:name="com.jixstreet.temanusaha.WaitingForConfirmationActivity" />
        <activity android:name="com.jixstreet.temanusaha.AppConfirmedActivity" />
        <activity android:name="com.jixstreet.temanusaha.AppRejectedActivity" />
        <activity android:name="com.jixstreet.temanusaha.AppStatusActivity" />
        <activity android:name="com.jixstreet.temanusaha.ForgotPasswordActivity" />
        <activity android:name="com.jixstreet.temanusaha.SignUpActivity" android:windowSoftInputMode="adjustPan" />

        <receiver
            android:name="com.google.android.gms.gcm.GcmReceiver"
            android:exported="true"
            android:permission="com.google.android.c2dm.permission.SEND" >
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
                <category android:name="com.jixstreet.temanusaha" />
            </intent-filter>
        </receiver>

        <!-- [START gcm_listener] -->
        <service
            android:name="com.jixstreet.temanusaha.gcm.MyGcmListenerService"
            android:exported="false" >
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
            </intent-filter>
        </service>
        <!-- [END gcm_listener] -->
        <!-- [START instanceId_listener] -->
        <service
            android:name="com.jixstreet.temanusaha.gcm.MyInstanceIDListenerService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.android.gms.iid.InstanceID"/>
            </intent-filter>
        </service>
        <!-- [END instanceId_listener] -->
        <service
            android:name="com.jixstreet.temanusaha.gcm.RegistrationIntentService"
            android:exported="false">
        </service>

        <!-- Goolge API Key -->
        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="AIzaSyB73WGpdTKXwUMSu98HWurqe8HNNta9Phc" />

    </application>

</manifest>
